            android:onClick="cbEye"
            android:text="Eye"
            android:visibility="visible" />

        <CheckBox
            android:id="@+id/cb_glasses"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_row="1"
            android:layout_column="1"
            android:onClick="cbGlasses"
            android:text="Glasses"
            android:visibility="visible" />

        <CheckBox
            android:id="@+id/cb_mustache"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_row="2"
            android:layout_column="1"
            android:onClick="cbMustache"
            android:text="Mustache"
            android:visibility="visible" />

        <CheckBox
            android:id="@+id/cb_nose"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_row="3"
            android:layout_column="1"
            android:onClick="cbNose"
            android:text="Nose"
            android:visibility="visible" />

        <CheckBox
            android:id="@+id/cb_shoes"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_row="4"
            android:layout_column="1"
            android:onClick="cbShoes"
            android:text="Shoes"
            android:visibility="visible" />
    </GridLayout>
