            android:onClick="cbEyebrow"
            android:text="Eyebrow"
            android:visibility="visible" />

        <!--        Second columns-->

        <CheckBox
            android:id="@+id/cb_eye"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_row="0"
